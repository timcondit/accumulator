{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::cgp-data-lake",
        "arn:aws:s3:::export.dev.skillfox",
        "arn:aws:s3:::sftp.prod.skillfox.com"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::cgp-data-lake/*",
        "arn:aws:s3:::export.dev.skillfox/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::sftp.prod.skillfox.com/*"
      ]
    }
  ]
}
